<Window x:Class="Tour.Admin.Functions.DeleteMng"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tour.Admin.Functions"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Удалить менеджера" Height="210" Width="414">
    <Grid Background="#FFF5F5F7">
        <Button Content="Удалить" HorizontalAlignment="Left" Margin="195,0,0,0" VerticalAlignment="Center" Height="30" Background="#FF08D27B" Foreground="#FFF5F5F7" FontWeight="Medium" BorderBrush="#FFF5F5F7" Width="120" Click="DeleteMng_Click"/>
        <ComboBox x:Name="Mng" HorizontalAlignment="Left" Margin="110,0,0,0" VerticalAlignment="Center" Width="78" BorderBrush="#FFE5E5EA" Height="30" Foreground="#FF2C2C2E" Background="#FFE5E5EA"/>
        <Label Content="Id:" HorizontalAlignment="Left" Margin="80,0,0,0" VerticalAlignment="Center" Width="25" Height="30"/>
    </Grid>
</Window>
