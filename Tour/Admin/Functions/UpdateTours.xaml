        <Label Content="Ecotravel" HorizontalAlignment="Left" Margin="25,4,0,0" VerticalAlignment="Top" FontSize="24" FontWeight="Bold" Width="117" RenderTransformOrigin="0.5,0.5" Foreground="White">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="0"/>
                    <RotateTransform/>
                    <TranslateTransform Y="0.101"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label Content="Редактировать туры" HorizontalAlignment="Left" Margin="303,72,0,0" VerticalAlignment="Top" FontSize="18" FontWeight="Bold" Width="194" Foreground="#FF2D2D2F" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="0.252"/>
                    <RotateTransform/>
                    <TranslateTransform Y="0.396"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label Content="Страна" FontWeight="Medium" HorizontalAlignment="Left" Margin="151,135,0,0" VerticalAlignment="Top" FontSize="14" Width="59" Foreground="#FF2D2D2F" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="0.252"/>
                    <RotateTransform/>
                    <TranslateTransform Y="0.396"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label Content="Город" FontWeight="Medium" HorizontalAlignment="Left" Margin="376,135,0,0" VerticalAlignment="Top" FontSize="14" Width="59" Foreground="#FF2D2D2F" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="0.252"/>
                    <RotateTransform/>
                    <TranslateTransform Y="0.396"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label Content="Тип отдыха" FontWeight="Medium" HorizontalAlignment="Left" Margin="577,135,0,0" VerticalAlignment="Top" FontSize="14" Width="84" Foreground="#FF2D2D2F" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="0.252"/>
                    <RotateTransform/>
                    <TranslateTransform Y="0.396"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label Content="Вид транспорта" FontWeight="Medium" HorizontalAlignment="Left" Margin="232,274,0,0" VerticalAlignment="Top" FontSize="14" Width="119" Foreground="#FF2D2D2F" RenderTransformOrigin="0.5,0.5">
