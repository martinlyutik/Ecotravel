<Window x:Class="Tour.Admin.Functions.UpdateTourFunctions.Delete.DeleteCountry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tour.Admin.Functions.UpdateTourFunctions.Delete"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Удалить страну" Height="200" Width="400">
    <Grid Background="#FFF5F5F7">
        <Button Content="Удалить" HorizontalAlignment="Left" Margin="216,0,0,0" VerticalAlignment="Center" Height="30" Background="#FF08D27B" Foreground="#FFF5F5F7" FontWeight="Medium" BorderBrush="#FFF5F5F7" Width="120" Click="DeleteCountry_Click"/>
        <ComboBox x:Name="Country" HorizontalAlignment="Left" Margin="134,0,0,0" VerticalAlignment="Center" Width="78" BorderBrush="#FFFFFCD6" Height="30">
            <ComboBox.Background>
                <LinearGradientBrush EndPoint="0,1">
                    <GradientStop Color="#FFF0F0F0"/>
                    <GradientStop Color="#FFFFFCD6" Offset="1"/>
                </LinearGradientBrush>
            </ComboBox.Background>
        </ComboBox>
        <Label Content="Название:" HorizontalAlignment="Left" Margin="65,0,0,0" VerticalAlignment="Center" Width="69" Height="30"/>

    </Grid>
</Window>
