<Window x:Class="Tour.Admin.Functions.UpdateTourFunctions.Insert.InsertCity"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tour.Admin.Functions.UpdateTourFunctions.Insert"
        mc:Ignorable="d"
        Title="Добавить город" Height="200" Width="400">
    <Grid Background="#FFF5F5F7">
        <ComboBox x:Name="Country" HorizontalAlignment="Left" Margin="160,67,0,0" VerticalAlignment="Top" Width="120" Height="30"/>
        <TextBox x:Name="City" HorizontalAlignment="Left" Margin="160,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="30"/>
        <Label Content="Город" HorizontalAlignment="Left" Margin="106,32,0,0" VerticalAlignment="Top"/>
        <Label Content="Страна" HorizontalAlignment="Left" Margin="106,69,0,0" VerticalAlignment="Top"/>
        <Button Content="Добавить" HorizontalAlignment="Left" Margin="135,109,0,0" VerticalAlignment="Top" Height="30" Background="#FF08D27B" Foreground="#FFF5F5F7" FontWeight="Medium" BorderBrush="#FFF5F5F7" Width="120" Click="InsertCity_Click"/>

    </Grid>
</Window>
