<Window x:Class="Tour.Admin.Functions.UpdateTourFunctions.Insert.InsertVidRazmesch"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tour.Admin.Functions.UpdateTourFunctions.Insert"
        mc:Ignorable="d"
        Title="Добавить вид размещения" Height="200" Width="400">
    <Grid>
        <TextBox x:Name="VidRazm" HorizontalAlignment="Center" Margin="0,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="130" Height="22"/>
        <Label Content="Вид размещения" HorizontalAlignment="Left" Margin="130,34,0,0" VerticalAlignment="Top"/>
        <Button Content="Добавить" HorizontalAlignment="Center" Margin="0,94,0,0" VerticalAlignment="Top" Height="25" Background="#FFD1EEFC" Width="130" Click="InsertVidRazm_Click"/>

    </Grid>
</Window>
