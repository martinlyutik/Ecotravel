<Window x:Class="Tour.Admin.Functions.UpdateTourFunctions.Insert.InsertCountry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tour.Admin.Functions.UpdateTourFunctions.Insert"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Closing="ClosingButton"
        Title="Добавить страну" Height="200" Width="400">
    <Grid Background="#FFF5F5F7">
        <TextBox x:Name="Country" HorizontalAlignment="Left" Margin="139,23,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="149" Height="30"/>
        <Label Content="Страна" HorizontalAlignment="Left" Margin="85,25,0,0" VerticalAlignment="Top"/>
        <Button Content="Добавить" HorizontalAlignment="Left" Margin="127,102,0,0" VerticalAlignment="Top" Height="30" Background="#FF08D27B" Foreground="#FFF5F5F7" FontWeight="Medium" BorderBrush="#FFF5F5F7" Width="120" Click="InsertCountry_Click"/>
        <ComboBox x:Name="Visa" HorizontalAlignment="Left" Margin="139,59,0,0" VerticalAlignment="Top" Width="149" Height="30"/>
        <Label Content="Виза" HorizontalAlignment="Left" Margin="85,59,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
