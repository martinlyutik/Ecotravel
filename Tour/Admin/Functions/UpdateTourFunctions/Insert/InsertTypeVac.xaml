<Window x:Class="Tour.Admin.Functions.UpdateTourFunctions.Insert.InsertTypeVac"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tour.Admin.Functions.UpdateTourFunctions.Insert"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Добавить тип отдыха" Height="200" Width="400">
    <Grid Background="#FFF5F5F7">
        <TextBox x:Name="Type" HorizontalAlignment="Left" Margin="171,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="30"/>
        <Label Content="Тип отдыха" HorizontalAlignment="Left" Margin="89,47,0,0" VerticalAlignment="Top" Height="26"/>
        <Button Content="Добавить" HorizontalAlignment="Left" Margin="125,84,0,0" VerticalAlignment="Top" Height="30" Background="#FF08D27B" Foreground="#FFF5F5F7" FontWeight="Medium" BorderBrush="#FFF5F5F7" Width="130" Click="InsertType_Click"/>

    </Grid>
</Window>
