<Window x:Class="Tour.Admin.Functions.DeleteUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tour.Admin.Functions"
        mc:Ignorable="d"
        Title="Удалить пользователя" Height="200" Width="400">
    <Grid>
        <Button Content="Удалить пользователя" HorizontalAlignment="Left" Margin="167,0,0,0" VerticalAlignment="Center" Height="30" Background="#FFD1EEFC" Width="152" Click="DeleteUser_Click"/>
        <ComboBox x:Name="User" HorizontalAlignment="Left" Margin="85,0,0,0" VerticalAlignment="Center" Width="78" BorderBrush="#FFFFFCD6" Height="30">
            <ComboBox.Background>
                <LinearGradientBrush EndPoint="0,1">
                    <GradientStop Color="#FFF0F0F0"/>
                    <GradientStop Color="#FFFFFCD6" Offset="1"/>
                </LinearGradientBrush>
            </ComboBox.Background>
        </ComboBox>
        <Label Content="Id:" HorizontalAlignment="Left" Margin="55,0,0,0" VerticalAlignment="Center" Width="25" Height="30"/>
    </Grid>
</Window>
